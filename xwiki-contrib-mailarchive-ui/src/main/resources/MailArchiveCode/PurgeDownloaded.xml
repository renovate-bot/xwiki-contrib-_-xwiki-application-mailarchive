#set ($confirm = $request.confirm)
#if ("$confirm"=="1")
#set($sql = ", BaseObject as obj where obj.name=doc.fullName and obj.className='MailArchiveCode.MailTopicClass' and obj.name not like 'MailArchiveCode.MailTopic%'")
#foreach ($item in $xwiki.searchDocuments($sql))
  * [[$item]] 
#set ($document = $xwiki.getDocument($item))
Deleting $document
$xwiki.getXWiki().deleteDocument($document.getDocument(), false, $context.getContext())
#end

#set($sql = ", BaseObject as obj where obj.name=doc.fullName and obj.className='MailArchiveCode.MailClass' and obj.name not like 'MailArchiveCode.MailClass%'")
